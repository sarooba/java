import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
         int num=0;
         Scanner scn = new Scanner(System.in);
         num=scn.nextInt();
         for(int i=1;i<=10;i++)
         {
             System.out.println(num+" x "+i+" = "+(num*i));
         }
    }
}
